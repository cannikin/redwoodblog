datasource DS {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

generator photonjs {
  provider = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

model Post {
  id        Int @id @default(autoincrement())
  title     String
  body      String
  createdAt DateTime @default(now())
}

model Contact {
  id        Int @id @default(autoincrement())
  name      String
  email     String
  message   String
  createdAt DateTime @default(now())
}
